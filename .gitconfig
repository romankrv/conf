[user]
        name = Roman Kalinichenko
        email = romankrv@gmail.com

[sendemail]
        smtpserver = smtp.gmail.com
        smtpserverport = 587
        smtpencryption = tls
        smtpuser = romankrv@gmail.com
[alias]
        st = status
        co = checkout
        d = diff
        br = branch
[core]
        editor = emacs -nw -Q

[push]
        default = current
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
        path =
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
